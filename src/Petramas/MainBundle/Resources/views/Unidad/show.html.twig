{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Unidad</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Marca</th>
                <td>{{ entity.marca }}</td>
            </tr>
            <tr>
                <th>Fechamantenimiento</th>
                <td>{{ entity.fechaMantenimiento|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Kilometraje</th>
                <td>{{ entity.kilometraje }}</td>
            </tr>
            <tr>
                <th>Tiempo</th>
                <td>{{ entity.tiempo|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('unidad') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('unidad_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
