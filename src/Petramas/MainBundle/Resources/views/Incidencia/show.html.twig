{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Incidencia</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Fechaincidencia</th>
                <td>{{ entity.fechaIncidencia|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fecharesolucion</th>
                <td>{{ entity.fechaResolucion|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Maquinaria</th>
                <td>{{ entity.maquinaria }}</td>
            </tr>
            <tr>
                <th>Observacion</th>
                <td>{{ entity.observacion }}</td>
            </tr>
            <tr>
                <th>Solucion</th>
                <td>{{ entity.solucion }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('incidencia') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('incidencia_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
